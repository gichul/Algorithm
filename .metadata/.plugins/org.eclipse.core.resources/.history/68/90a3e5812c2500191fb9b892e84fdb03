package Day14;
import java.util.*;

public class P1158 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int N,M;
		int count=0;
		N=sc.nextInt();
		M=sc.nextInt();
		int[] J=new int[N+1];
		Arrays.fill(J, 1);
		ArrayList<Integer> ans=new ArrayList<>();
		
		//i는 index를 나타낸다.
		int i=1;
		while(count!=N) {
			int check=0;
				while(check!=M) {
					if(J[i]==0) {
						i++;
						continue;
					}
					ans.add(J[i]);
					check++;
				}			
			count++;
		}
		
		
	}

}
